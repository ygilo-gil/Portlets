#confirmationTable {
	width: 100%;
	font-size: 15px;
	font-weight: 500;
	text-shadow: 1px 1px 1px white;
	border: 1px solid #E0E0E0;
}

#confirmationTable td {
	padding: 8px;
}

#confirmationTable tr:nth-child(odd) {
	background: #E4E4E4;
}

#confirmationTable tr:nth-child(even) {
	background: #FFFFFF;
}